{
    "name": "ezsystems/ezplatform-rest",
    "description": "eZ Platform REST bundle",
    "type": "ezplatform-bundle",
    "license": "GPL-2.0-only",
    "autoload": {
        "psr-4": {
            "EzSystems\\EzPlatformRestBundle\\": "src/bundle/",
            "EzSystems\\EzPlatformRest\\": "src/lib/",
            "EzSystems\\EzPlatformRestBundle\\Tests\\": "tests/bundle/",
            "EzSystems\\EzPlatformRest\\Tests\\": "tests/lib/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {"type": "vcs", "url": "https://github.com/webhdx/ezpublish-kernel.git"},
        {"type": "vcs", "url": "https://github.com/ezsystems/doctrine-dbal-schema.git"},
        {"type": "vcs", "url": "https://github.com/webhdx/JMSTranslationBundle.git"},
        {"type": "vcs", "url": "https://github.com/webhdx/Routing.git"}
    ],
    "require": {
        "php": "^7.3",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "ext-xmlwriter": "*",
        "ezsystems/ezpublish-kernel": "dev-symfony5 as 8.0.x-dev",
        "symfony/http-kernel": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/routing": "^5.0",
        "symfony/http-foundation": "^5.0",
        "symfony/config": "^5.0",
        "symfony/yaml": "^5.0",
        "symfony/event-dispatcher": "^5.0",
        "symfony/security-csrf": "^5.0",
        "symfony/expression-language": "^5.0",
        "symfony/form": "^5.0",
        "symfony/security-bundle": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "ezsystems/ezplatform-code-style": "^0.1.0",
        "friendsofphp/php-cs-fixer": "^2.16.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "nyholm/psr7": "^1.1",
        "symfony/http-client": "^5.0",
        "symfony/browser-kit": "^5.0"
    },
    "scripts": {
        "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
    },
    "extra": {
        "_ezplatform_branch_for_behat_tests": "master",
        "branch-alias": {
            "dev-tmp_ci_branch": "1.0.x-dev",
            "dev-master": "1.0.x-dev"
        }
    }
}
