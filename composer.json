{
    "name": "ezsystems/ezplatform-rest",
    "description": "eZ Platform REST bundle",
    "type": "ezplatform-bundle",
    "license": "GPL-2.0-only",
    "autoload": {
        "psr-4": {
            "eZ\\Bundle\\EzPublishRestBundle\\": "eZ/Bundle/EzPublishRestBundle/",
            "eZ\\Publish\\Core\\REST\\Common\\": "eZ/Publish/Core/REST/Common/",
            "eZ\\Publish\\Core\\REST\\Server\\": "eZ/Publish/Core/REST/Server/"
        }
    },
    "require": {
        "php": "^7.1",
        "ezsystems/ezpublish-kernel": "^8.0@dev",
        "ezsystems/doctrine-dbal-schema": "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.5.11",
        "friendsofphp/php-cs-fixer": "~2.15.0"
    },
    "scripts": {
        "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
